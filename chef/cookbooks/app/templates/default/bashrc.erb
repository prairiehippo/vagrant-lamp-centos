# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
        . /etc/bashrc
fi

# User specific aliases and functions

export PATH="/etc/composer/vendor/bin:$PATH"

alias drush='sudo -u apache drush'
alias drushme=/etc/composer/vendor/bin/drush
alias ll='ls -al'
alias ..='cd ..'
alias ...='cd ../..'